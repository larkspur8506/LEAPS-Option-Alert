# 请复制此文件为 .env 并填入真实的配置值
# cp .env.example .env

# 管理员密码（设置后请修改）
ADMIN_PASSWORD=your_admin_password_here

# Polygon.io API Key（从 https://polygon.io/ 获取）
POLYGON_API_KEY=your_polygon_api_key_here

# 企业微信 Webhook URL（从企业微信群机器人设置中获取）
WECHAT_WEBHOOK_URL=https://qyapi.weixin.qq.com/cgi-bin/webhook/send?key=your_webhook_key_here

# QQQ 规则开关
QQQ_RULE_A_ENABLED=true
QQQ_RULE_B_ENABLED=true
QQQ_RULE_C_ENABLED=true
QQQ_RULE_D_ENABLED=true

# 期权规则配置
MAX_HOLDING_DAYS=270
TAKE_PROFIT_PHASE1_THRESHOLD=0.50
TAKE_PROFIT_PHASE1_DAYS=120
TAKE_PROFIT_PHASE2_THRESHOLD=0.30
TAKE_PROFIT_PHASE2_DAYS=180
TAKE_PROFIT_PHASE3_THRESHOLD=0.10
STOP_LOSS_THRESHOLD=0.30
DTE_WARNING_DAYS=45

# 数据清理配置
ALERT_LOG_RETENTION_DAYS=90
DAILY_QQQ_DATA_RETENTION_DAYS=30
